{{#each orders as |order|}}


{{#if (ownerCheck order.owner._id)}}
<section data-id="{{order._id}}">
<div class="card text-grey bg-light col-12">
  <h6>Order Time</h6>
  <h3>
    {{dateFormat order.createdAt}}
  </h3>

  <h6>Items</h6>
  <h3>
    {{order.items.length}}
  </h3>
</div>
</section>
{{/if}}


{{/each}}
